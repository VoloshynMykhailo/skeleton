{
  "name": "webpack-tutorial",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack --env param1=first param2=second param3=3 --config ./webpack/development.config",
    "start:node": "cross-env NODE_ENV=development node --require @babel/register ./webpack/scripts/development.js",
    "start:node:crossPlatform": "cross-env NODE_ENV=development node \"./webpack/scripts/development.js\"",
    "build": "webpack --config ./webpack/production.config",
    "build:node": "cross-env NODE_ENV=production node --require @babel/register ./webpack/scripts/production.js",
    "serve": "serve ./dist",
    "analyze": "webpack-bundle-analyzer \"./dist/stats.json\""
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
