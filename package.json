    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@svgr/webpack": "^5.5.0",
    "app-root-path": "^3.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "detect-port-alt": "^1.1.6",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
